<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="apple-mobile-web-app-capable" content="yes" />
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent" />
    <meta name="description" content="WebAppVueWebpack." />
    <title>
        @{
            if (ViewBag.Title != "")
            {
                <text>@ViewBag.Title / WebAppVueWebpack</text>
    }
    else
    {
                <text>WebAppVueWebpack</text>
    }
}
    </title>
    <link type="image/x-icon" href="@Url.Content("~/favicon.ico")" rel="shortcut icon" />
    <link rel="apple-touch-icon" href="@Url.Content("~/Images/apple-touch-icon/apple-touch-icon.png")" />
    <link rel="apple-touch-icon" sizes="72x72" href="@Url.Content("~/Images/apple-touch-icon/apple-touch-icon-72x72-precomposed.png")" />
    <link rel="apple-touch-icon" sizes="114x114" href="@Url.Content("~/Images/apple-touch-icon/apple-touch-icon-114x114-precomposed.png")" />
    
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic,700,700italic,800italic,800" rel="stylesheet" type="text/css" />
    
    @Styles.Render("~/bundles/style")
</head>
<body>
    <div class="container">
        @RenderBody()
    </div>

    @Scripts.Render("~/bundles/scripts")
    @RenderSection("scripts", required: false)
</body>
</html>
